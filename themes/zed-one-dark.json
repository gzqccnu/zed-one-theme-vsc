{
  "name": "ZED One Dark",
  "type": "dark",
  "colors": {
    "editor.background": "#282C33",
    "editor.foreground": "#DCE0E5",
    "editorCursor.foreground": "#74ADE8",
    "editor.lineHighlightBackground": "#2F343EBF",
    "editorLineNumber.foreground": "#C8CCD459",
    "editor.selectionBackground": "#74ADE83D",
    "editor.inactiveSelectionBackground": "#B477CF3D",
    "editorWhitespace.foreground": "#C8CCD40D",
    "editorBracketMatch.background": "#74ADE81A",
    "editorBracketMatch.border": "#74ADE8",
    "editor.findMatchBackground": "#74ADE866",
    "editor.findMatchHighlightBackground": "#74ADE81A",
    "activityBar.background": "#2F343E",
    "sideBar.background": "#2F343E",
    "statusBar.background": "#3B414D",
    "titleBar.activeBackground": "#3B414D",
    "tab.activeBackground": "#282C33",
    "tab.inactiveBackground": "#2F343E",
    "tab.activeForeground": "#DCE0E5",
    "tab.inactiveForeground": "#878A98",
    "tab.activeBorder": "#74ADE8",
    "panel.background": "#2F343E",
    "input.background": "#2E343E",
    "input.foreground": "#DCE0E5",
    "input.border": "#363C46"
  },
  "tokenColors": [
    {
      "scope": ["keyword.control.import", "storage.modifier", "storage.type", "keyword.control"],
      "settings": { "foreground": "#B477CF" }
    },
    {
      "scope": ["string", "text.literal", "string.quoted.double"],
      "settings": { "foreground": "#A1C181" }
    },
    {
      "scope": ["variable", "variable.language", "variable.parameter"],
      "settings": { "foreground": "#a9afbcff" }
    },
    {
      "scope": ["constant.numeric", "constant.language", "constant.character"],
      "settings": { "foreground": "#BF956A" }
    },
    {
      "scope": ["entity.name.function"],
      "settings": { "foreground": "#74ADE8" }
    },
    {
      "scope": ["meta.function-call", "meta.method-call"],
      "settings": { "foreground": "#D07277" }
    },
    {
      "scope": ["entity.name.class", "entity.name.type", "support.class"],
      "settings": { "foreground": "#6EB4BF" }
    },
    {
      "scope": ["punctuation.bracket", "punctuation.definition.block", "punctuation.definition.bracket", "punctuation.definition.parameters"],
      "settings": { "foreground": "#6EB4BF" }
    },
    {
      "scope": ["comment"],
      "settings": { "foreground": "#5D636F", "fontStyle": "italic" }
    },
    {
      "scope": ["punctuation.definition.tag", "entity.name.tag"],
      "settings": { "foreground": "#74ADE8" }
    },
    {
      "scope": ["meta.decorator", "entity.name.function.decorator"],
      "settings": { "foreground": "#74ADE8" }
    },
    {
      "scope": ["storage.type.function", "keyword.operator"],
      "settings": { "foreground": "#6EB4BF" }
    },

    {
      "scope": ["markup.heading", "markup.heading.setext", "markup.heading.markdown"],
      "settings": { "foreground": "#74ADE8", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.bold", "markup.bold.markdown"],
      "settings": { "foreground": "#B477CF", "fontStyle": "bold" }
    },
    {
      "scope": ["markup.italic", "markup.italic.markdown"],
      "settings": { "foreground": "#D07277", "fontStyle": "italic" }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code.block", "markup.raw.block"],
      "settings": { "foreground": "#BF956A", "background": "#2F343E80" }
    },
    {
      "scope": ["markup.link", "markup.underline.link", "string.other.link"],
      "settings": { "foreground": "#6EB4BF", "fontStyle": "underline" }
    },
    {
      "scope": ["markup.quote", "punctuation.definition.quote"],
      "settings": { "foreground": "#A1C181", "fontStyle": "italic" }
    },
    {
      "scope": ["markup.list", "markup.list.numbered", "markup.list.unnumbered"],
      "settings": { "foreground": "#D07277" }
    },
    {
      "scope": ["markup.separator"],
      "settings": { "foreground": "#5D636F" }
    }
  ],
  "semanticTokenColors": {
    "function.declaration": { "foreground": "#74ADE8" },
    "method.declaration": { "foreground": "#74ADE8" },
    "function": { "foreground": "#D07277" },
    "method": { "foreground": "#D07277" },
    "class": { "foreground": "#6EB4BF" },
    "type": { "foreground": "#6EB4BF" },
    "parameter": { "foreground": "#C8CCD4" },
    "keyword": { "foreground": "#B477CF" },
    "string": { "foreground": "#A1C181" },
    "decorator": { "foreground": "#74ADE8" },
    "decorator.identifier": { "foreground": "#74ADE8" },
    "punctuation": { "foreground": "#6EB4BF" }
  },
  "semanticHighlighting": true
}
